    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^3.1.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8",
    "bs-platform": "^5.0.6"
  }
}
